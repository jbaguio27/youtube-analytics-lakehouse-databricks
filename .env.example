# Local development configuration (non-secret defaults).
# Copy this file to .env and adjust values for your environment.

# Databricks CLI
DATABRICKS_CONFIG_PROFILE=<your-databricks-config-profile>
DATABRICKS_HOST=<your-databricks-host>

# Project defaults
YOUTUBE_ANALYTICS_CATALOG=youtube_analytics_dev
YOUTUBE_ANALYTICS_SCHEMA_BRONZE=bronze
YOUTUBE_ANALYTICS_SCHEMA_SILVER=silver
YOUTUBE_ANALYTICS_SCHEMA_GOLD=gold

# SQL bootstrap helper (raw warehouse id only)
DATABRICKS_SQL_WAREHOUSE_ID=<your-databricks-sql-warehouse-id>

# YouTube OAuth local bootstrap (one-time)
YOUTUBE_OAUTH_CLIENT_ID=<your-google-oauth-client-id>
YOUTUBE_OAUTH_CLIENT_SECRET=<your-google-oauth-client-secret>
YOUTUBE_OAUTH_REFRESH_TOKEN=<your-google-oauth-refresh-token>
YOUTUBE_OAUTH_REDIRECT_HOST=127.0.0.1
YOUTUBE_OAUTH_REDIRECT_PORT=8765

# Databricks secret scope for YouTube credentials
DATABRICKS_SECRET_SCOPE=youtube-analytics

# dbt local runtime (for local dbt commands)
DBT_DATABRICKS_HOST=https://dbc-xxxxxxxx-xxxx.cloud.databricks.com
DBT_DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/<your-sql-warehouse-id>
DBT_DATABRICKS_TOKEN=<your-databricks-pat>

ALERT_EMAIL=<your-alert-email>