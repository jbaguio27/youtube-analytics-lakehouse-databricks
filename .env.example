# Local development configuration (non-secret defaults).
# Copy this file to .env and adjust values for your environment.

# Databricks CLI
DATABRICKS_CONFIG_PROFILE=<your-databricks-config-profile>
DATABRICKS_HOST=<your-databricks-host>

# Project defaults
YOUTUBE_ANALYTICS_CATALOG=youtube_analytics_dev
YOUTUBE_ANALYTICS_SCHEMA_BRONZE=bronze
YOUTUBE_ANALYTICS_SCHEMA_SILVER=silver
YOUTUBE_ANALYTICS_SCHEMA_GOLD=gold

# SQL bootstrap helper (raw warehouse id only)
DATABRICKS_SQL_WAREHOUSE_ID=<your-databricks-sql-warehouse-id>
